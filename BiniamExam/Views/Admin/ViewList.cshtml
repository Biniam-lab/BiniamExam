
@model IEnumerable<BiniamExam.Models.Customer>
@{
    ViewData["Title"] = "ViewList";
}

<h1>Account List</h1>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.password)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in UserData.customers)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.email)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.password)
                </td>
            </tr>
        }
    </tbody>
</table>
<a class="btn btn-info" asp-controller="Admin" asp-action="Index">Back to dashboard</a>